    "@angular/core": "^5.2.0",
    "@angular/forms": "^5.2.0",
    "@angular/http": "^5.2.0",
    "@angular/material": "^5.2.4",
    "@angular/platform-browser": "^5.2.0",
    "@angular/platform-browser-dynamic": "^5.2.0",
    "@angular/router": "^5.2.0",
    "@angular/service-worker": "^5.2.0",
    "angularfire2": "^5.0.0-rc.4",
    "clipboard": "^2.0.0",
    "core-js": "^2.4.1",
    "firebase": "^4.8.0",
    "hammerjs": "^2.0.8",
    "moment": "^2.21.0",
    "moment-timezone": "^0.5.14",
    "mp.css": "^1.0.0",
